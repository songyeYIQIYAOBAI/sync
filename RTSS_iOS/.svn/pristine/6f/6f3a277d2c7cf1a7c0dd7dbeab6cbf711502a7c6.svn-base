#import <Foundation/Foundation.h>

@interface TTRule : NSObject

@property (nonatomic, copy) NSString *mRuleId;
@property (nonatomic, assign) long long mOrgAmount; //unit price target
@property (nonatomic, assign) long long mTargetAmount; //unit price source
@property (nonatomic, assign) long long mMinOrgAmount; //0
@property (nonatomic, assign) long long mMaxOrgAmount; //total of source
@property (nonatomic, assign) long long mChargeAmount;


@property (nonatomic, copy) NSString* mName;
@property (nonatomic, assign) long long mChargeMeasureId;
@property (nonatomic, assign) int mOrgItemType; //unit source
@property (nonatomic, copy) NSString *mOrgItem; //resource item id
@property (nonatomic, assign) int mOrgMeasureId; //unit source
@property (nonatomic, assign) int mTargetItemType; //unit target
@property (nonatomic, copy) NSString *mTargetItem;
@property (nonatomic, assign) int mTargetMeasureId;
@property (nonatomic, assign) int mValidityType;
@property (nonatomic, assign) int mDurationType;
@property (nonatomic, assign) int mDuration;

@end
